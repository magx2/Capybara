package /misc

import /capybara/type

struct SimpleStruct {
	int: Int
	float: Float
	string: String
}

struct ExtendedStruct {
	new_int: Int
	boolean: Boolean
	...SimpleStruct
}

fun new_simple_struct() = SimpleStruct {float = 1.1, int = 1, string = "s"}

fun new_extended_struct() = ExtendedStruct {
	int = 1,
	string = "s",
	new_int = 2,
	float = 1.1,
	boolean = true
}

